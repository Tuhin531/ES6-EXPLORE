const numbers = [12, 13, 24, 35, 6, 5];
function getDouble(numbers) {

    const outPut = [];
    for (const number of numbers) {
        const double = number * 2;
        outPut.push(double);
    }
    return outPut;
}
const result = getDouble(numbers);
// console.log(result);

const makeDouble = numbers.map(num => num * 2);
// console.log(makeDouble);

const makeFiveTime = [1, 2, 3, 4, 5, 6,].map(x => x * 5);
console.log(makeFiveTime)